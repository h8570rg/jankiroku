{
  "name": "jankiroku",
  "version": "0.2.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "22.x"
  },
  "volta": {
    "node": "22.18.0"
  },
  "scripts": {
    "prepare": "husky",
    "dev": "next dev -p 3001",
    "build": "next build",
    "start": "next start -p 3002",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "type": "tsc",
    "test": "vitest run",
    "spell": "cspell . --gitignore",
    "pre-commit": "npm run lint && npm run type && npm run test && npm run spell && lint-staged",
    "supabase:start": "env-cmd -f .env.local supabase start",
    "supabase:stop": "env-cmd -f .env.local supabase stop",
    "supabase:login": "supabase login",
    "supabase:reset": "env-cmd -f .env.local supabase db reset",
    "supabase:diff": "env-cmd -f .env.local supabase db diff -f local",
    "supabase:squash": "supabase migration squash",
    "supabase:type": "env-cmd -f .env.local supabase gen types typescript --local > lib/database.types.ts",
    "migrate": "tsc --skipLibCheck --esModuleInterop  --outDir lib/migration/dist lib/migration/index.ts && node lib/migration/dist/migration/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/h8570rg/jankiroku.git"
  },
  "author": "https://github.com/h8570rg",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/h8570rg/jankiroku/issues"
  },
  "homepage": "https://github.com/h8570rg/jankiroku#readme",
  "dependencies": {
    "@dnd-kit/sortable": "10.0.0",
    "@heroui/react": "2.8.2",
    "@hookform/resolvers": "5.2.1",
    "@supabase/ssr": "0.7.0",
    "@supabase/supabase-js": "2.56.0",
    "classnames": "2.5.1",
    "dayjs": "1.11.14",
    "framer-motion": "12.23.12",
    "next": "15.5.2",
    "next-themes": "0.4.6",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-hook-form": "7.62.0",
    "recharts": "2.15.4",
    "sharp": "0.34.3",
    "swr": "2.3.6",
    "use-debounce": "10.0.5",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@types/react": "19.1.12",
    "@types/react-dom": "19.1.9",
    "autoprefixer": "10.4.21",
    "cspell": "9.2.1",
    "env-cmd": "10.1.0",
    "eslint": "9.34.0",
    "eslint-config-next": "15.5.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-tailwindcss": "3.18.2",
    "husky": "9.1.7",
    "lint-staged": "16.1.5",
    "postcss": "8.5.6",
    "prettier": "3.6.2",
    "supabase": "2.39.2",
    "tailwindcss": "3.4.17",
    "typescript": "5.9.2",
    "typescript-eslint": "8.41.0",
    "vitest": "3.2.4"
  }
}
